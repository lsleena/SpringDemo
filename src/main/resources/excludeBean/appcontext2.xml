<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans-4.0.xsd
    http://www.springframework.org/schema/context
    http://www.springframework.org/schema/context/spring-context.xsd">

    <context:annotation-config/>

    <bean id="dollar" class="com.leena.spring.autoWiring.excludeBean.DollarPayment" autowire-candidate="false" />

    <bean id="card" class="com.leena.spring.autoWiring.excludeBean.CardPayment" />

    <bean id="payServiceBean" class="com.leena.spring.autoWiring.excludeBean.MoneyPayServiceImpl">
    </bean>

</beans>